		 cat logfile.$$$$; \
		 echo "$(subdir)/8-1 compile FAILED; SKIPPING"; \
		(echo "$(subdir)/8-1 compile FAILED; SKIPPING"; cat logfile.$$$$) >> $(LOGFILE); \
	fi; \
	rm -f logfile.$$$$

